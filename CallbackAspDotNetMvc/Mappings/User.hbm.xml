<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="CallbackAspDotNetMvc.Models" assembly="CallbackAspDotNetMvc">
  <class name="User" table="User2">
    <id name="ID" unsaved-value="0">
      <column name="ID" not-null="true" />
      <generator class="identity"/>
    </id>
    <property name="created_at" />
    <property name="updated_at" />
    <property name="telephone" unique="true" not-null="true" type="string" length="12"/>
    <property name="code" type="string" length="5"/>
    <property name="lastname" type="string" length="20"/>
    <property name="firstname" type="string" length="20"/>
    <property name="secondname" type="string" length="20"/>
    <property name="result" type="string" length="1000"/>
    
    
  </class>
</hibernate-mapping>